{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["ES2020"],
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "outDir": "dist-electron",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["electron/**/*"]
}
